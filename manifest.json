{
  "name": "Ad Tracker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Parse GPT requests",
  "content_scripts": [
    {
      "matches": ["http://localhost:3000/*", "https://*.ft.com/*"],
      "js": ["dist/intercept-requests.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["http://localhost:3000/*", "https://*.ft.com/*"],
      "js": ["dist/inspect-slots.js"],
      "run_at": "document_idle"
    }
  ]
}
